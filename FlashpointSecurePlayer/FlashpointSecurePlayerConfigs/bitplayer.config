<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="flashpointSecurePlayer" type="FlashpointSecurePlayer.Shared+FlashpointSecurePlayerSection, FlashpointSecurePlayer, Version=2.1.1.0, Culture=neutral, PublicKeyToken=null" />
	</configSections>
	
	<flashpointSecurePlayer>
		<templates>
			<template name="bitmagic">
				<mode name="SOFTWARE" commandLine="BitPlayer\Player.exe &quot;%FP_HTDOCS_FILE%&quot;" />
				<modifications>
					<registryStates binaryType="SCS_32BIT_BINARY">
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1" valueName="MainDir" value="%FP_STARTUP_PATH%\BitMagic" valueKind="String" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1" valueName="NewEdition" value="0" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1" valueName="Update" value="1" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1" valueName="UpdatePostponed" value="0" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\ScreenSaver" valueName="BitSaverSoundEnabled" value="0" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\ScreenSaver" valueName="ScreenSaverBmp" value="%FP_STARTUP_PATH%\BitMagic\Skins\screensaver.jpg" valueKind="String" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="SoundSchemeName" value="Default" valueKind="String" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="SkinName" value="BitPlayer Small 1.2" valueKind="String" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="ConnectionType" value="1" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="ProxyEnabled" value="1" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="ProxyHost" value="127.0.0.1" valueKind="String" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="ProxyPort" value="22500" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="SystemtrayEnabled" value="1" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="StatuswindowEnabled" value="1" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="ScreensaverEnabled" value="0" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="PlayerEnabled" value="0" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="ParentalLockEnabled" value="0" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="DisplayQuality" value="1" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="SkinFile" value="%FP_STARTUP_PATH%\BitMagic\Skins\default.bmm" valueKind="String" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="SoundSchemeFile" value="%FP_STARTUP_PATH%\BitMagic\Sounds\default.bss" valueKind="String" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="PartnerID" value="0" valueKind="Dword" />
						<registryState type="VALUE" keyName="HKEY_CURRENT_USER\Software\BitMagic\1\SystemSettings" valueName="InetPreConfig" value="0" valueKind="Dword" />
					</registryStates>
				</modifications>
			</template>
		</templates>
	</flashpointSecurePlayer>
	
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
	</startup>
</configuration>